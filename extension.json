{
  "identifier": "aleksandrbeliaev.SelectLineContents",
  "name": "Select Line Contents",
  "organization": "Aleksandr Beliaev",
  "description": "Quickly select line contents.",
  "version": "1.0.0",
  "categories": ["commands"],
  "bugs": "https://github.com/certainlyakey/SelectLineContents.novaextension/issues",
  "main": "main.js",

  "commands": {
    "editor": [
      {
        "title": "Select Line Contents",
        "command": "selectLineContents.selectLineContents",
        "shortcut": "cmd-opt-l",
        "when": "editorHasFocus"
      }
    ]
  }
}
